class Solution {
    public String reverseWords(String s) {
        String[] sc = s.split(" ");
        StringBuilder sb = new StringBuilder();
        for(int i=sc.length-1;i>=0;i--){
            if(sc[i].equals(""))
                continue;
            sb.append(sc[i].trim()+" ");
        }
        return sb.toString().trim();
    }
}
