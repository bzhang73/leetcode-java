public static int uniquePairs(int[] nums, int target){
    Set<Integer> set = new HashSet<>();
    Set<Integer> already=new HashSet<>();
    int res=0;
    for(int num: nums){
        if(set.contains(target-num) && !already.contains(num)){
            res++;
            already.add(num();
            already.add(target-num);
            
        }else{
            set.add(num);
        }
    }
    return res;
}
